{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./build",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "express"]
  },
  "include": ["**/*.ts"],
  "typeRoots": ["**/*.ts"],
  "exclude": ["node_modules"]
}
